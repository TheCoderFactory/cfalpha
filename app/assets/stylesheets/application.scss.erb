@import "materialize/components/color";
$primary-color: color("blue", "") !default;
/*$footer-color: color("yellow", "") !default;*/
@import 'materialize';
@import 'YTPlayer';
@import 'logo';

body {
    background: #EEE;
    display: flex;
    min-height: 100vh;
    flex-direction: column;
    font-family: "futura-pt","dapifer", garamond, serif;
}
h1,h2,h3,h4,h5,h6 {
  font-family: 'clavo', roboto, sans-serif;
  font-style: italic;
}
footer.page-footer {
    margin-top: 0px;
    padding-top: 20px;
    /*background-color: ;*/
    @extend .red;
}
@media only screen and (min-width: 993px) {
.container {
    width: 85%;
}}
@media only screen and (min-width: 1200px) {
html {
    font-size: 16px;
}
}
.pagination a li.active {
    color: #fff;
}
nav {
  background-color: rgba(0,0,0,0.2);
}
#customElement {
            width: 100%;
            height: 600px;
            background: rgba(0, 0, 0, 1);
            position: relative;
            top: 0;
            left: 0;
            z-index: 0;
            margin-top: -80px;
            color: rgba(255,255,255,0.8);
            padding-top: 15%;
        }
#top-section {
  margin-top: -80px;
  padding-top: 80px;
}
.hero-title {
  font-size: 6em;
  text-transform: uppercase;
}
.nav-logo {
  padding-top: 15px;
}

span.badge {
  right: inherit;
}
.parallax-container {
      height: 600px;
    }


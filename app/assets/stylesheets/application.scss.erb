.blue-container {
    @extend .blue, .lighten-2;
  }
 .lime-container {
 	@extend .lime, .lighten-2;
 }
.teal-container {
    @extend .teal, .lighten-2;
  }
.black-container {
	@extend .yellow, .lighten-4;
}
nav.top-nav {
    box-shadow: none;
}

nav.top-nav a.page-title {
    line-height: 122px;
    font-size: 48px;
}
nav ul a {
    font-size: 1rem;
    color: #fff;
    display: block;
    padding: 0 15px;
    text-transform: uppercase;
}

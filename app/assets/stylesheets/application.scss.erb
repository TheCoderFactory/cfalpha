@import "materialize/components/color";
$primary-color: color("light-blue", "") !default;
@import 'materialize';
@import 'YTPlayer';

body {
    background: #EEE;
    display: flex;
    min-height: 100vh;
    flex-direction: column;
    font-family: "proxima-nova","dapifer", garamond, serif;
}
h1,h2,h3,h4,h5,h6 {
  font-family: 'minerva-modern', roboto, sans-serif;
}
.white-form input {
  background: white;
}
footer.page-footer {
    margin-top: 0px;
    padding-top: 20px;
    background-color: #1f2b3a;
}
.transparent-card {
  background-color: rgba(0,0,0,0.3);
  border: 2px white solid;
}
.transparent-card h4 {
  font-size: 24px;
  line-height: 20px;
  margin-top: 0;
}
.transparent-button {
  background-color: rgba(0,0,0,.02);
  border: 1px white solid;
}
.content {
  font-family: 'proxima-nova';
  padding-bottom: 8px;
}
@media only screen and (min-width: 1200px) {
html {
    font-size: 16px;
}
}
.partner-logo {
  background-color: white;
}
.btn:hover, .btn-large:hover {
    @extend .light-blue
}


.pagination a li.active {
    color: #fff;
}

.capitals {
  text-transform: uppercase;
}
nav {
  background-color: rgba(0,0,0,0.2);
}
.card .card-action a {
    color: #03A9F4;
    }
.card .card-action a:hover {
    color: #333;
    }
#customElement {
    width: 100%;
    height: 800px;
    background: rgba(0, 0, 0, 1);
    position: relative;
    top: 0;
    left: 0;
    z-index: 0;
    margin-top: -80px;
    color: rgba(255,255,255,0.8);
    padding-top: 10%;
    /*background-image: url(<%= @video_background %>); */
}
.top-bar, .top-bar a {
  color: white;
  padding:0px;
}
#top-section {
  margin-top: -80px;
  padding-top: 80px;
}
.hero-title {
  font-size: 6em;
  text-transform: uppercase;
}


span.badge {
  right: inherit;
}
.parallax-container {
      height: 600px;
    }


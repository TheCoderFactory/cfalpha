@import "materialize/components/color";
$primary-color: color("light-blue", "") !default;
@import 'materialize';
@import 'YTPlayer';

body {
    background: #EEE;
    display: flex;
    min-height: 100vh;
    flex-direction: column;
    font-family: "proxima-nova","dapifer", garamond, serif;
}
h1,h2,h3,h4,h5,h6 {
  font-family: 'minerva-modern', roboto, sans-serif;
}
.collapsible-body p {
    margin: 0;
    padding: 5px 20px;
}
.fa-ul {
  padding-left: 10px;
}
.collapsible-body {
  padding: 10px 40px;
}
.bold {
  font-weight: 700;
}
.course-type {
  background-color: white; 
  font-size: 1em;
  padding: 5px;
  font-family: "proxima-nova", serif;
}
footer.page-footer {
    margin-top: 0px;
    padding-top: 20px;
    background-color: #1f2b3a;
}
.transparent-card {
  background-color: rgba(0,0,0,0.3);
  border: 2px white solid;
}
.transparent-card h4 {
  font-size: 24px;
  line-height: 20px;
  margin-top: 0;
}
.transparent-button {
  background-color: rgba(0,0,0,.02);
  border: 1px white solid;
}
.tagline {
  padding: 10px;
}
.content {
  font-family: 'proxima-nova';
  padding-bottom: 8px;
}
@media only screen and (min-width: 1200px) {
html {
    font-size: 16px;
}
}
#tech-heroes {
  background: url(<%= asset_path 'heroes2.jpg' %>);
  height: 400px;
  background-size: cover;
}
#header-boxes {
  display: flex;
  align-content: stretch;
  align-items: stretch;
}
.header-box {
  padding-top: 10px;
  flex: 1;
  height: 220px;
}
.header-box:first-child { display: flex; }
.header-box:last-of-type { flex: 3;}

.blog-post h1 {
  margin: 5px;
  font-size: 30px;
}
.partner-logo {
  background-color: white;
}
.btn:hover, .btn-large:hover {
    @extend .light-blue
}

.community {
  background-image: url(http://res.cloudinary.com/coder-factory/image/upload/v1418034058/heart_fivwmy.jpg);
  background-size: cover;
  background-repeat: none;
  height: 800px;
}
.pagination a li.active {
    color: #fff;
}

.capitals {
  text-transform: uppercase;
}
nav {
  background-color: rgba(0,0,0,0.5);
}
.card .card-action a {
    color: #03A9F4;
    }
.card .card-action a:hover {
    color: #333;
    }
#customElement {
    width: 100%;
    height: 800px;
    background: rgba(0, 0, 0, 1);
    position: relative;
    top: 0;
    left: 0;
    z-index: 0;
    margin-top: -80px;
    color: rgba(255,255,255,0.8);
    padding-top: 15%;
    /*background-image: url(<%= @video_background %>); */
}
.top-bar, .top-bar a {
  color: white;
  padding:0px;
}
#top-section {
  margin-top: -80px;
  padding-top: 80px;
}
.hero-title {
  font-size: 6em;
  text-transform: uppercase;
}


span.badge {
  right: inherit;
}
.parallax-container {
      height: 600px;
    }


.blue-container {
    @extend .blue, .lighten-2;
  }
 .lime-container {
 	@extend .lime, .lighten-2;
 }
.teal-container {
    @extend .teal, .lighten-2;
  }
.black-container {
	@extend .yellow, .lighten-4;
}
nav.top-nav {
    box-shadow: none;
}

nav.top-nav a.page-title {
    line-height: 122px;
    font-size: 48px;
}
nav ul a {
    font-size: 1rem;
    color: #fff;
    display: block;
    padding: 0 15px;
    text-transform: uppercase;
}


@media only screen and (min-width : 601px) {
  nav, nav .nav-wrapper i, nav a.button-collapse, nav a.button-collapse i {
    height: 80px;
    line-height: 80px; }
    .brand-logo {
      padding-top: 10px;
    }
  .navbar-fixed {
    height: 80px; } }
main {
  flex: 1 0 auto;
}
#my-map-canvas img {
  max-width:none!important;
  background:none!important;
}
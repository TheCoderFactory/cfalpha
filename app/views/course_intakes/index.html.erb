<p id="notice"><%= notice %></p>

<h1>Listing Course Intakes</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Course location</th>
      <th>Start date</th>
      <th>Start time</th>
      <th>End date</th>
      <th>End time</th>
      <th>Days of week</th>
      <th>Status</th>
      <th>Price</th>
      <th>Slug</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_intakes.each do |course_intake| %>
      <tr>
        <td><%= course_intake.course %></td>
        <td><%= course_intake.course_location %></td>
        <td><%= course_intake.start_date %></td>
        <td><%= course_intake.start_time %></td>
        <td><%= course_intake.end_date %></td>
        <td><%= course_intake.end_time %></td>
        <td><%= course_intake.days_of_week %></td>
        <td><%= course_intake.status %></td>
        <td><%= course_intake.price %></td>
        <td><%= course_intake.slug %></td>
        <td><%= link_to 'Show', course_intake %></td>
        <td><%= link_to 'Edit', edit_course_intake_path(course_intake) %></td>
        <td><%= link_to 'Destroy', course_intake, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course intake', new_course_intake_path %>

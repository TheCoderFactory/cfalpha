<p id="notice"><%= notice %></p>

<h1>Listing Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Picture</th>
      <th>Phone</th>
      <th>Github</th>
      <th>Twitter</th>
      <th>Linkedin</th>
      <th>Gender</th>
      <th>Age</th>
      <th>Referral</th>
      <th>Referral other</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.picture %></td>
        <td><%= profile.phone %></td>
        <td><%= profile.github %></td>
        <td><%= profile.twitter %></td>
        <td><%= profile.linkedin %></td>
        <td><%= profile.gender %></td>
        <td><%= profile.age %></td>
        <td><%= profile.referral %></td>
        <td><%= profile.referral_other %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>

  <div class="row">
    <div class="col s12 m12">
      <div class="card z-depth-3 hoverable">
        <div class="card-content grey-text text-darken-2">
        <p id="notice"><%= notice %></p>

<h1>Listing Feedback Forms</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Course location</th>
      <th>Expectations</th>
      <th>Recommend</th>
      <th>Another</th>
      <th>Satisfaction</th>
      <th>Value</th>
      <th>Best part</th>
      <th>Comments</th>
      <th>Testimonial</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedback_forms.each do |feedback_form| %>
      <tr>
        <td><%= feedback_form.course %></td>
        <td><%= feedback_form.course_location %></td>
        <td><%= feedback_form.expectations %></td>
        <td><%= feedback_form.recommend %></td>
        <td><%= feedback_form.another %></td>
        <td><%= feedback_form.satisfaction %></td>
        <td><%= feedback_form.value %></td>
        <td><%= feedback_form.best_part %></td>
        <td><%= feedback_form.comments %></td>
        <td><%= feedback_form.testimonial %></td>
        <td><%= link_to 'Show', feedback_form %></td>
        <td><%= link_to 'Edit', edit_feedback_form_path(feedback_form) %></td>
        <td><%= link_to 'Destroy', feedback_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback form', new_feedback_form_path %>

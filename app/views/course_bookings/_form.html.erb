<%= simple_form_for(@course_booking) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :course_intake %>
    <%= f.association :user %>
    <%= f.input :price %>
    <%= f.input :promo_code %>
    <%= f.input :paid %>
    <%= f.input :completed %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
